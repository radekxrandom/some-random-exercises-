using System;
 
namespace fib
{
    class Program
    {
  static int calcFib(int n)
        {
 
            if (n==0 || n == 1)
            {
                return n;
            }
            return calcFib(n - 1) + calcFib(n - 2);
 
        }
 
        static void Main(string[] args)
        {
            Console.WriteLine(" "+ calcFib(5));
        }
    }
}
